<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="SupakullTrackerServices" namespace="SupakullTrackerServices">
  <class name="UserLinkDAO" table="USER_LINK">
    <id name="UserLinkId" column="LINK_ID" generator="assigned" />
    <property name="Owner" column="OWNER" type="System.Boolean" />
    <property name="UserId" column="USER_ID" type="System.Int64" />
    <property name="ServiceAccountId" column="SERVICE_ACCOUNT_ID" type="System.Int32" />
    <many-to-one name="Account" class="ServiceAccountDAO" column="SERVICE_ACCOUNT_ID" />
  </class>
</hibernate-mapping>
