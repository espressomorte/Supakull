<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="SupakullTrackerServices" namespace="SupakullTrackerServices">
<class name="ServiceAccountDAO" table="SERVICE_ACCOUNT">
  <id name="ServiceAccountId" column="SERVICE_ID" generator="assigned" />
  <property name="ServiceAccountName" column="ACCOUNT_NAME" />
  <property name="Source" column="SOURCE_ID" />
  
  <bag cascade="none" name="MappingTemplates" table="TEMPLATE">
    <key column="SERVICE_ID" />
    <one-to-many class="TemplateDAO"/>
  </bag>

  <map name="FirstToken" inverse="true"  table="TOKEN" >
    <key  column="TOKEN_ID"/>
    <index column="KYE" type="System.String"/>
    <element column="VALUE" type="System.String" length="1500"/>
  </map>

  <map name="SecondToken" table="TOKEN">
    <key column="TOKEN_ID" foreign-key="TOKEN_ID2" />
    <index column="KYE" type="System.String"/>
    <element column="VALUE" type="System.String" length="1500"/>
  </map>

  <map name="ThirdToken" table="TOKEN">
    <key column="TOKEN_ID" foreign-key="TOKEN_ID3" />
    <index column="KYE" type="System.String"/>
    <element column="VALUE" type="System.String" length="1500"/>
  </map>

  <map name="FourthToken" table="TOKEN">
    <key column="TOKEN_ID" foreign-key="TOKEN_ID4" />
    <index column="KYE" type="System.String"/>
    <element column="VALUE" type="System.String" length="1500"/>
  </map>
  
  <map name="FifthToken" table="TOKEN">
    <key column="TOKEN_ID" foreign-key="TOKEN_ID5" />
    <index column="KYE" type="System.String"/>
    <element column="VALUE" type="System.String" length="1500" />
  </map>
</class>
</hibernate-mapping>
